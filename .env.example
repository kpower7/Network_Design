# Environment Configuration for SCM275x Facility Location Optimization
# Copy this file to .env and add your actual configuration values

# Gurobi Optimization License
# Obtain academic license from: https://www.gurobi.com/academia/academic-program-and-licenses/
GUROBI_LICENSE_FILE=path/to/your/gurobi.lic
GRB_LICENSE_FILE=path/to/your/gurobi.lic

# Data Sources Configuration
DATA_PATH=./data/
OUTPUT_PATH=./outputs/

# Optimization Parameters
DEFAULT_TIME_LIMIT=3600
DEFAULT_MIP_GAP=0.01
DEFAULT_THREADS=4

# Mapping and Visualization
MAPBOX_ACCESS_TOKEN=your_mapbox_token_here_optional
OPENSTREETMAP_TILES=https://tile.openstreetmap.org/{z}/{x}/{y}.png

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/optimization.log

# Performance Settings
MEMORY_LIMIT_GB=8
PARALLEL_PROCESSING=True
CACHE_RESULTS=True

# Development Settings
DEBUG=False
VERBOSE_OUTPUT=True
